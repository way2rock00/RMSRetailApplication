        <Item Value="poNumber"/>
      </AttrNames>
    </list>
    <list IterBinding="buyerLovIterator" ListOperMode="navigation" ListIter="buyerLovIterator" id="buyerLov"
          DTSupportsMRU="true" SelectItemValueMode="ListObject">
      <AttrNames>
        <Item Value="buyerName"/>
        <Item Value="buyerNumber"/>
