<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="HomeTF">
    <default-activity>Login</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>TestMB</managed-bean-name>
      <managed-bean-class>com.deloitte.rmsapp.service.TestMB</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__6">
      <managed-bean-name>pageControl</managed-bean-name>
      <managed-bean-class>com.deloitte.rmsapp.mobile.pageControl</managed-bean-class>
      <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <view id="HeaderListPage">
      <page>/pages/Supplier/POHeadersList.amx</page>
    </view>
    <view id="SearchPage">
      <page>/pages/Supplier/POSearchPG.amx</page>
    </view>
    <view id="Login">
      <page>/Login.amx</page>
    </view>
    <view id="DetailsPage">
      <page>/pages/Supplier/PODetailsPG.amx</page>
    </view>
    <view id="SummaryPage">
      <page>/pages/Supplier/SummaryPG.amx</page>
    </view>
    <view id="NotificationsPage">
      <page>/pages/Supplier/NotificationsPG.amx</page>
    </view>
    <control-flow-rule id="__2">
      <from-activity-id>SearchPage</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>openPO</from-outcome>
        <to-activity-id>HeaderListPage</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__17">
        <from-outcome>notification</from-outcome>
        <to-activity-id>NotificationsPage</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__23">
        <from-outcome>home</from-outcome>
        <to-activity-id>SummaryPage</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>Login</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>LandingPage</from-outcome>
        <to-activity-id>SummaryPage</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>HeaderListPage</from-activity-id>
      <control-flow-case id="__8">
        <from-outcome>openPODetail</from-outcome>
        <to-activity-id>DetailsPage</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__11">
        <from-outcome>search</from-outcome>
        <to-activity-id>SearchPage</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__16">
        <from-outcome>notification</from-outcome>
        <to-activity-id>NotificationsPage</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__21">
        <from-outcome>home</from-outcome>
        <to-activity-id>SummaryPage</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__9">
      <from-activity-id>DetailsPage</from-activity-id>
      <control-flow-case id="__10">
        <from-outcome>search</from-outcome>
        <to-activity-id>SearchPage</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__15">
        <from-outcome>notification</from-outcome>
        <to-activity-id>NotificationsPage</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__22">
        <from-outcome>home</from-outcome>
        <to-activity-id>SummaryPage</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__12">
      <from-activity-id>SummaryPage</from-activity-id>
      <control-flow-case id="__13">
        <from-outcome>search</from-outcome>
        <to-activity-id>SearchPage</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__14">
        <from-outcome>notification</from-outcome>
        <to-activity-id>NotificationsPage</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__24">
        <from-outcome>toOpenPO</from-outcome>
        <to-activity-id>HeaderListPage</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__18">
      <from-activity-id>NotificationsPage</from-activity-id>
      <control-flow-case id="__19">
        <from-outcome>home</from-outcome>
        <to-activity-id>SummaryPage</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__20">
        <from-outcome>search</from-outcome>
        <to-activity-id>SearchPage</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
